{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:has_component",
    "component": "minecraft:enchantments",
    "ignore_default": true,
    "on_true": {
      "type": "minecraft:condition",
      "property": "minecraft:fishing_rod/cast",
      "on_true": {
        "type": "minecraft:select",
        "property": "minecraft:display_context",
        "cases": [
          {
            "when": [
              "firstperson_righthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "item/enchanted_fishing_rod_cast_hand_first_right"
            }
          },
          {
            "when": [
              "firstperson_lefthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "item/enchanted_fishing_rod_cast_hand_first_left"
            }
          },
          {
            "when": [
              "gui",
              "ground",
              "fixed"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/fishing_rod"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "item/enchanted_fishing_rod_cast_hand"
        }
      },
      "on_false": {
        "type": "minecraft:select",
        "property": "minecraft:display_context",
        "cases": [
          {
            "when": [
              "firstperson_righthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "item/enchanted_fishing_rod_uncast_hand_first_right"
            }
          },
          {
            "when": [
              "firstperson_lefthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "item/enchanted_fishing_rod_uncast_hand_first_left"
            }
          },
          {
            "when": [
              "gui",
              "ground",
              "fixed"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/fishing_rod"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "item/enchanted_fishing_rod_uncast_hand"
        }
      }
    },
    "on_false": {
      "type": "minecraft:condition",
      "property": "minecraft:fishing_rod/cast",
      "on_true": {
        "type": "minecraft:select",
        "property": "minecraft:display_context",
        "cases": [
          {
            "when": [
              "firstperson_lefthand",
              "firstperson_righthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "item/fishing_rod_cast_hand"
            }
          },
          {
            "when": [
              "gui",
              "ground",
              "fixed"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/fishing_rod_cast"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "item/fishing_rod_cast_hand"
        }
      },
      "on_false": {
        "type": "minecraft:select",
        "property": "minecraft:display_context",
        "cases": [
          {
            "when": [
              "firstperson_lefthand",
              "firstperson_righthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "item/fishing_rod_uncast_hand"
            }
          },
          {
            "when": [
              "gui",
              "ground",
              "fixed"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/fishing_rod"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "item/fishing_rod_uncast_hand"
        }
      }
    }
  }
}