			"body2.rz": "     cos(-0.8 +var.ls )/10*var.m*(1-var.inair) +( sin( var.ft/6 +sin( var.ft/3 )*1.2 )*pi/2 )*var.inair",
			"body2.ty": "-5.5    +cos( var.waves )*(1-var.inair)                                   +(-cos( var.ft )/3   +clamp( ( var.position-pos_y )*4,-1,1)*4 +2-1*2 )*clamp(1-(1-var.inair)*4,0,1) +if( is_in_water, 0, -2.5*(1) )",
			"body2.ry": "  (  sin( 0.8 +var.ls )/10*(0.3+var.m  ) +torad(var.hy)/2 )*(1-var.inair) +( sin( var.ft )/4   +clamp( ( var.position-pos_y )*2,-1,1)*body2.rz )*var.inair",
			"body3.ry": "  ( -cos( 0.8 +var.ls )/8 *(1+var.m*1.3) +torad(var.hy)/2 )*(1-var.inair) +(-cos( var.ft )/4   -clamp( ( var.position-pos_y )*3,-1,1)*body2.rz )*var.inair",
			"head2.ry": "  (   -body2.ry                          -torad(var.hy)/3 )*(1-var.inair) +( sin( var.ft )/3   +clamp( ( var.position-pos_y )*2,-1,1)*body2.rz )*var.inair",
			"tail2.ry": "  ( -cos(      var.ls )/4 *(1+var.m    ) +torad(var.hy)   )*(1-var.inair) +(-sin( var.ft )/1.5 -clamp( ( var.position-pos_y )*2,-1,1)*body2.rz )*var.inair",
			"fin_top.ry": "( -cos( 0.8 +var.ls )/16*(1+var.m    )                  )*(1-var.inair) +(-cos( var.ft )/6                                                   )*var.inair",
			"fin_top.rz": "( -cos(      var.ls )/7 *(1+var.m    )                  )*(1-var.inair) +(-sin( var.ft )/2                                                   )*var.inair",
			"body2.tx": "  (  cos( 0.8 +var.ls )/3 *(1+var.m*1.3)                  )*(1-var.inair)   +if( is_in_water, 0, -0.8*(1) )",
			"body3.rx": "cos( pi/4 +var.waves )/16*(1-var.inair)",
			"fin_top.rx": "body3.rx/2",
			"body2.sx": "1 +( 0.5-random(id) )/3",
			"body2.sy": "body2.sx",
			"body2.sz": "body2.sx"
		},
		{
			"left_fin.rx": " torad( 20)",
			"right_fin.rx": "torad( 20)",
			"left_fin.ry": " torad(-20) +( -cos( 0.5 +var.r +limb_swing/2 +age/9 +sin( var.r +limb_swing/2 +age/18 )/2 )/2 *max(0,1-var.m*2) )*(1-var.inair)",
			"right_fin.ry": "torad( 20) +(  sin( 0.5 +var.r +limb_swing/2 +age/9 +cos( var.r +limb_swing/2 +age/18 )/2 )/2 *max(0,1-var.m*2) )*(1-var.inair)",
			"left_fin.rz": " torad( 35) +(  cos(      var.r +limb_swing/2 +age/9 +sin( var.r +limb_swing/2 +age/18 )   )/2 *max(0,1-var.m*2) +( 0.35 -cos(var.ls)/5 ) *min(1,var.m*2) )*(1-var.inair) +sin( age/1.5 )/2*var.inair",
